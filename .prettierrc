{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 1000,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "strict"
}
