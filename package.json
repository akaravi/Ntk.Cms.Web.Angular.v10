  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pwa": "ng serve --configuration=production",
    "pwa-dist": "npx http-server -p 8080 -c-1 dist/<project-name>/browser",
    "serve1": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve",
    "serve2": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve",
    "serve3": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng serve",
    "serve4": "node --max_old_space_size=32768 ./node_modules/@angular/cli/bin/ng serve",
    "build-prod": "ng build --configuration production",
    "build-prod2": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production",
    "build-prod3": "node --max_old_space_size=32768 ./node_modules/@angular/cli/bin/ng build --configuration production --build-optimizer",
    "format": "prettier --write \"src/**/*.{ts,js,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,html,css,scss,json}\"",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,js}\" --max-warnings 0"
  },
  "prettier": {
    "overrides": [
